config olsrd
	# uncomment the following line to use a custom config file instead:
	#option config_file '/etc/olsrd.conf'

	#option IpVersion '6and4'
	# if 6and4 does not work make sure this patch is applied: https://dev.openwrt.org/ticket/4803
	option IpVersion '6and4'

config LoadPlugin
	option library 'olsrd_mdns.so.1.0.0'
	option "NonOlsrIf"  "eth0.0"

config Interface
	list interface 'ninuxif'

config 'Hna4'
	option 'netaddr' '192.168.1.0'
	option 'netmask' '255.255.255.0'

#config 'Hna6'
#	option 'netaddr' '2002::'
#	option 'prefix' '64'
